{
    "name": "Tab Garden",
    "description": "Display garden on the new tab page",
    "version": "0.1",
    "incognito": "split",
    "content_security_policy": "script-src 'self' https://code.jquery.com; object-src 'self'",
    "chrome_url_overrides": {"newtab": "start-page.html"},
    "permissions": ["bookmarks", "history", "storage"],
    "manifest_version": 2,
    "icons": {
        "512" : "icons/icon.png"
    }
  }


